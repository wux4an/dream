{> with project_name as String
{> with project_description as String
{> with task_list as String
<section>
    <header>
        <h1>{{ project_name }}</h1>
        <button id="add-task-btn" onclick="insertNewTask()">+</button>
        <p>{{ project_description }}</p>
    </header>
    {{ task_list }}
    <footer>
        <a href="/">Back to All Todos</a>
    </footer>
</section>